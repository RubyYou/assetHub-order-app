{
    "name": "CarehubAPP",
    "displayName": "CarehubAPP",
    "version": "1.0.0",
    "description": "A sample Apache Cordova application that responds to the deviceready event.",
    "author": "Apache Cordova Team",
    "license": "Apache-2.0",
    "keywords": [
        "ios",
        "browserify",
        "framework7",
        "f7",
        "vue",
        "vues",
        "material",
        "mobile",
        "app"
    ],
    "scripts": {
        "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o www/js/build.js",
        "serve": "http-server -o -c 1 -a localhost",
        "dev": "npm-run-all --parallel watchify serve",
        "build": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o www/css/build.css ] -e src/main.js | uglifyjs -c warnings=false -m > www/js/build.js",
        "sw": "workbox generateSW"
    },
    "browser": {
        "vue": "vue/dist/vue.common.js"
    },
    "browserify": {
        "transform": [
            "babelify",
            "vueify"
        ],
        "global": true
    },
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "bufferutil": "^3.0.3",
        "cordova-android": "^6.2.3",
        "cordova-ios": "^4.5.2",
        "cordova-plugin-compat": "^1.2.0",
        "cordova-plugin-whitelist": "^1.3.2",
        "es6-promise": "^4.2.2",
        "firebase": "^4.5.0",
        "framework7": "^1.6.5",
        "framework7-vue": "^0.9.4",
        "moment": "^2.20.1",
        "socket.io-client": "^2.0.4",
        "utf-8-validate": "^4.0.0",
        "vue": "^2.2.6",
        "vue-socket.io": "^2.1.1-a",
        "vue-template-compiler": "^2.2.6",
        "vuex": "^3.0.0"
    },
    "devDependencies": {
        "babel-core": "^6.0.0",
        "babel-plugin-transform-runtime": "^6.0.0",
        "babel-preset-es2015": "^6.0.0",
        "babel-preset-stage-2": "^6.0.0",
        "babel-runtime": "^6.0.0",
        "babelify": "^7.2.0",
        "browserify": "^13.1.0",
        "browserify-css": "^0.9.2",
        "browserify-hmr": "^0.3.1",
        "cross-env": "^2.0.0",
        "envify": "^3.4.1",
        "http-server": "^0.9.0",
        "node-sass": "^4.7.2",
        "npm-run-all": "^2.3.0",
        "proxyquireify": "^3.0.1",
        "uglify-js": "^2.5.0",
        "vueify": "^9.0.0",
        "watchify": "^3.4.0",
        "workbox-cli": "^3.0.0"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-camera": {
                "CAMERA_USAGE_DESCRIPTION": "your usage message",
                "PHOTOLIBRARY_USAGE_DESCRIPTION": "your usage message"
            }
        },
        "platforms": [
            "android",
            "ios"
        ]
    }
}
