                    "customOptions": [
                        "A面",
                        "B面"
                    ]
                },
                {
                    "customComponent": {
                        "name": "RepeatInput",
                        "input": {"max": 70, "min": 0, "step": 0.5, "type": "number", "toFixed": 2}
                    }
                }
            ]
        },
